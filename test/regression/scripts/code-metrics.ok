#!/bin/sh
# Automatically generated file
# Source file example/code-metrics.sh

export SGDIR=/tmp/sg-$$
rm -rf $SGDIR
trap 'rm -rf "$SGDIR"' 0
trap 'exit $?' 1 2 3 15
mkdir $SGDIR

mkfifo  \
$SGDIR/npi-1.0.0 \
$SGDIR/npvo-FNAMELEN \
$SGDIR/npi-1.1.0 \
$SGDIR/npi-2.0.0 \
$SGDIR/npi-3.0.0 \
$SGDIR/npvo-NSTRUCT \
$SGDIR/npi-3.1.0 \
$SGDIR/npvo-NTYPEDEF \
$SGDIR/npi-3.2.0 \
$SGDIR/npvo-NVOID \
$SGDIR/npi-3.3.0 \
$SGDIR/npvo-NGETS \
$SGDIR/npi-3.4.0 \
$SGDIR/npvo-IDLEN \
$SGDIR/npi-2.1.0 \
$SGDIR/npvo-CHLINESCHAR \
$SGDIR/npi-2.2.0 \
$SGDIR/npvo-NCCHAR \
$SGDIR/npi-2.3.0 \
$SGDIR/npvo-NCOMMENT \
$SGDIR/npi-2.4.0 \
$SGDIR/npvo-NCOPYRIGHT \
$SGDIR/npi-4.0.0 \
$SGDIR/npi-5.0.0 \
$SGDIR/npvo-NCFILE \
$SGDIR/npi-5.1.0 \
$SGDIR/npvo-NCDIR \
$SGDIR/npi-4.1.0 \
$SGDIR/npi-6.0.0 \
$SGDIR/npvo-CLINESCHAR \
$SGDIR/npi-6.1.0 \
$SGDIR/npi-7.0.0 \
$SGDIR/npvo-NFUNCTION \
$SGDIR/npi-7.1.0 \
$SGDIR/npvo-NGOTO \
$SGDIR/npi-7.2.0 \
$SGDIR/npvo-NREGISTER \
$SGDIR/npi-7.3.0 \
$SGDIR/npvo-NMACRO \
$SGDIR/npi-7.4.0 \
$SGDIR/npvo-NINCLUDE \
$SGDIR/npi-7.5.0 \
$SGDIR/npvo-NCONST \
$SGDIR/npvo-NHFILE
#!/usr/local/bin/sgsh
